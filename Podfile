
source 'https://github.com/CocoaPods/Specs.git' #指定源 不写默认官方源
platform:ios, '9.0' #指定版本
inhibit_all_warnings! #去掉警告

def pods
    pod 'AFNetworking', '4.0.1'
    pod 'IQKeyboardManager'
    pod 'MBProgressHUD'
    pod 'SDWebImage'
    pod 'FMDB'
    
    pod 'YYModel'
    pod 'YYText'
    pod 'YYCategories'

    pod 'WMPageController'
    
    pod 'TZImagePickerController'
    pod 'MJRefresh'
    pod 'Masonry'
end


target "TemplateObject" do

pods  #使用宏

end

target "TemplateSwift" do

pods  #使用宏

end
